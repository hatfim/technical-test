    "@vitest/ui": "^1.5.0",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.5.4",
    "happy-dom": "^14.7.1",
    "nuxt": "3.11.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
